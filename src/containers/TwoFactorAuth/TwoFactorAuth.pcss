/* New banner adjustment - for individual item only */

.three-banner {
  &-container {
    width: 100%;
    display: -webkit-flex;
    display: flex;
    flex-wrap: wrap;
    margin-bottom: 10px;
    p, h1, h2, h3, h4 {
      padding: 0;
      margin: 0;
    }
    #banner-1 {
      .three-banner {
        &-overlay-image {
          background: url('./screens/LandingScreen/three-banner-1-overlay.png');
        }
      }
      .content {
        &-logo {
          background: url('./screens/LandingScreen/three-banner-1-logo.png');
        }
      }
    }
    #banner-2 {
      .three-banner {
        &-overlay-image {
          background: url('./screens/LandingScreen/three-banner-2-overlay.png');
        }
      }
      .content {
        &-logo {
          background: url('./screens/LandingScreen/three-banner-2-logo.png');
          margin-left: 10px;
        }
      }
    }
    #banner-3 {
      .three-banner {
        &-overlay-image {
          background: url('./screens/LandingScreen/three-banner-3-overlay.png');
        }
      }
    }
  }
  &-item {
    box-shadow: 0px 0px 15px rgba(0,0,0,.15);
    border: solid 1px rgba(0,0,0,0.1);
    border-radius: 10px;
    width: 32%;
    height: 205px;
    display: -webkit-flex;
    display: flex;
    flex-flow: row wrap;
    margin-right: 2%;
    overflow: hidden;
  }
  &-item:nth-child(3n+0) {
    margin-right: 0;
  }
  &-item:hover {
    opacity: 0.8;
  }
  &-background {
    width: 100%;
    height: 100%;
  }
  &-overlay {
    &-container {
      width: 100%;
      height: 100%;
      display: -webkit-flex;
      display: flex;
      justify-content: flex-end;
      position: relative;
      top: -100%;
    }
    &-image {
      width: 100%;
      height: 100%;
      background-size: contain !important;
      background-repeat: no-repeat !important;
      background-position: 100% 0 !important;
    }
  }
  &-content {
    width: 100%;
    height: 100%;
    position: relative;
    top: -200%;
    color: black;
    display: -webkit-flex;
    display: flex;
    flex-flow: row wrap;
    .content {
      &-logo {
        width: 100%;
        height: 100%;
        background-size: contain !important;
        background-repeat: no-repeat !important;
        background-position: 0 50% !important;
      }
      &-button {
        width: 85px;
        height: 25px;
        background: #823179;
        color: white;
        display: -webkit-flex;
        display: flex;
        align-items: center;
        justify-content: center;
      }
      &-col-1 {
        width: 30%;
        height: 100%;
        display: -webkit-flex;
        display: flex;
        align-items: center;
      }
      &-col-2 {
        width: 70%;
        height: 100%;
        padding-left: 20px;
        padding-right: 60px;
        display: -webkit-flex;
        display: flex;
        flex-flow: column wrap;
      }
      &-row-1 {
        height: 68%;
        width: 100%;
        display: -webkit-flex;
        display: flex;
        align-items: flex-end;
        color: #823179;
        padding-bottom: 10px;
        font-size: 16px;
        font-weight: 300;
        span {
          font-size: 24px;
          font-weight: 700;
        }
      }
      &-row-2 {
        height: 32%;
        width: 100%;
      }
    }
  }
}