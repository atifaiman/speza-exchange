.LandingTableContainer {
  -webkit-overflow-scrolling: touch;
  display: blcok;
  height: 450px;
  overflow-y: scroll;
  overflow-x: hidden;
  scrollbar-color: #6C6C6C #f0f0f0;
  scroll-margin: 0;
  scrollbar-width: thin;
  scrollbar-height: 2px;
  /*
  background-image: linear-gradient(to bottom, #ffffff, rgba(255, 255, 255, 0)), linear-gradient(to top, #ffffff, rgba(255, 255, 255, 0)), linear-gradient(to bottom, #aaa, rgba(255, 255, 255, 0)), linear-gradient(to top, #aaa, rgba(255, 255, 255, 0));
  background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ffffff), color-stop(100%, rgba(255, 255, 255, 0))), -webkit-gradient(linear, 50% 100%, 50% 0%, color-stop(0%, #ffffff), color-stop(100%, rgba(255, 255, 255, 0))), -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #aaa), color-stop(100%, rgba(255, 255, 255, 0))), -webkit-gradient(linear, 50% 100%, 50% 0%, color-stop(0%, #aaa), color-stop(100%, rgba(255, 255, 255, 0)));
  background-image: -moz-linear-gradient(top, #ffffff, rgba(255, 255, 255, 0)), -moz-linear-gradient(bottom, #ffffff, rgba(255, 255, 255, 0)), -moz-linear-gradient(top, #aaa, rgba(255, 255, 255, 0)), -moz-linear-gradient(bottom, #aaa, rgba(255, 255, 255, 0));
  scroll-margin: 0;
  background-position: 0 0, 0 100%, 0 0, 0 100%;
  background-repeat: no-repeat;
  background-color: white;
  background-size: 100% 20px, 100% 20px, 100% 10px, 100% 10px;
  background-attachment: local, local, scroll, scroll;
  */
  &::-webkit-scrollbar {
    width: 3px;
  } 
  &::-webkit-scrollbar-track {
    background: #fff;
    border: 1px solid transparent;
    background-clip: content-box;  
    opacity: 0.2;
  }
  &::-webkit-scrollbar-thumb {
    width: 3px;
    -webkit-border-radius: 5px;
    border-radius: 6px;
    background: #6C6C6C;
    background-size: 50px 50px;
  }
  &::-webkit-scrollbar-thumb:window-inactive {
    background: #6C6C6C; 
  } 
}


