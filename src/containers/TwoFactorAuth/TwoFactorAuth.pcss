.nav-tabs {
    border-bottom: none;
}
.nav-tabs>li>a, .nav-tabs>li>a:focus, .nav-tabs>li>a:hover {
  background-color: transparent;
  border: none;
  color: #aeaeae;
  font-size: 16px;
  font-weight: 400;
  @media screen and (max-width: 768px) {
    font-size: 14px;
  }
  @media screen and (max-width: 430px) {
    font-size: 14px;
  }
}
.nav-tabs>li>a:hover  {
  font-weight: bold;
  border-bottom: solid 0px #BF357F !important;
  color: #BF357F !important;
}
.nav-tabs > li.active > a {
  margin-bottom:2px;
  border: none !important;
  background: transparent !important;
  border-bottom: solid 0px #BF357F !important;
  color: #BF357F !important;
  font-weight: bold;
}

