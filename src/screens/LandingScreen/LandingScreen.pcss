.slider-content { 
 background-size: 100% 100% !important;
}

#change{
    color:#000;
    font-family:arial;
    padding:5px;
    transition:opacity .5s linear;
}
.hide{
    opacity:0;
}


.SLiderButton {
    cursor: pointer !important;
    background: linear-gradient(90.1deg, #BE55AD 0%, #543D8F 100.04%) !important;
    border-radius: 1px !important;
    border: none;
    position: absolute;
    bottom: 20%;
    left: 20%;
    &:hover {
        background: #E5E5E5 !important;
    }
    &:selected {
        background: linear-gradient(0deg, rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.1)), linear-gradient(180deg, #462871 0%, #2C194E 100%) !important;
    }
}