.LandingScreen{
    font-size: 15px;
}
.pg_landing {
  overflow: hidden;
  background: #FFFFFF;
}
.bannerThreeSlider {
    padding: 3px 0;
}
.ThreebannersMobile {
    margin: auto;
    width: 431px;
    height: 205px;
    box-shadow: 0px 0px 15px rgba(0,0,0,.15);
    border: solid 1px rgba(0,0,0,0.1);
    @media screen and (max-width: 768px) {
            display: none;
            width: 394px;
            height:190px; 
    }

    &:hover {
       opacity: 0.85;
    }
}
.ThreebannersMobileButtomPage {
    display: none;
    margin: auto;
    width: 100%;
    box-shadow: 0px 0px 15px rgba(0,0,0,.15);
    border: solid 1px rgba(0,0,0,0.1);
    @media screen and (max-width: 768px) {
        display: block;
        margin: 30px auto;
        height: 335px; 
    }
    @media screen and (max-width: 640px) {
        display: block;
        margin: 30px auto;
        height: 276px !important; 
    }
    @media screen and (max-width: 520px) {
        display: block;
        height: 226px !important; 
    }
    @media screen and (max-width: 430px) {
        display: block;
        height: 192px !important; 
    }
    @media screen and (max-width: 384px) {
        display: block;
        height: 171px !important; 
    }
     @media screen and (max-width: 340px) {
        display: block;
        height: 147px !important; 
    }
    &:hover {
       opacity: 0.85;
    }
}

.landpage-Banners {
    margin: auto;
    display: block;
    @media screen and (max-width: 768px) {
        display: none;
    }
}
.container_banners{
    width: 1200px;
    margin: auto;
    @media screen and (min-width: 1200px) {
        max-width: 1200px;
        min-width: 1200px;
    }
    @media screen and (max-width: 768px) {
        width: 90%;
        min-width: 85%;
    }
    @media screen and (max-width: 420px) {
        width: 95%;
        min-width: 85%;
    }
}
